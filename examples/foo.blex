circuit DLatch(d,clk)
  out RSLatch(d' and clk, d and clk)
end

# This RS latch is properly handled.
circuit RSLatch(r,s)
  P = r nor Q
  Q = s nor P
  out P, Q
end


circuit main(d,clk)
  out DLatch(d,clk)
end
