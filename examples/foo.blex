circuit Alpha(z)
  A = z
  out A
end

circuit Beta(z)
  i,j = k, DLatch(true, true)
  out z
end

# This RS latch is properly handled.
circuit RSLatch(r,s)
  P = r nor Q
  Q = s nor P
  out P, Q
end

circuit DLatch(d,clk)
  out RSLatch(d' and clk, d and clk)
end

circuit main(d,clk)
  out DLatch(d,clk)
end
